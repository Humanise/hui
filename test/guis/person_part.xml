<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../xml/setup.xsl"?>
<gui xmlns="uri:hui" padding="10" title="Box">
	<structure>
		<top>
			<toolbar>
				<icon icon="common/stop" text="Annuler"/>
				<icon icon="common/delete" text="Slet"/>
				<icon icon="common/save" text="Gem"/>
				<divider/>
				<icon icon="common/person" text="Vælg person"/>
				<item label="Justering">
					<dropdown>
						<option value="left" text="Venstre"/>
						<option value="left" text="Højre"/>
					</dropdown>
				</item>
				<divider/>
				<grid>
					<row>
						<cell right="5"><checkbox/><label>Fornavn</label></cell>
						<cell right="5"><checkbox/><label>Mellemnavn</label></cell>
						<cell right="5"><checkbox/><label>Efternavn</label></cell>
					</row>
					<row>
						<cell right="5"><checkbox/><label>Initialer</label></cell>
						<cell right="5"><checkbox/><label>Kaldenavn</label></cell>
						<cell right="5"><checkbox/><label>Køn</label></cell>
					</row>
				</grid>
				<divider/>
				<grid>
					<row>
						<cell right="5"><checkbox/><label>Gade</label></cell>
						<cell right="5"><checkbox/><label>By</label></cell>
					</row>
					<row>
						<cell right="5"><checkbox/><label>Postnummer</label></cell>
						<cell right="5"><checkbox/><label>Land</label></cell>
					</row>
				</grid>
			</toolbar>
		</top>
		<middle></middle>
	</structure>
</gui>