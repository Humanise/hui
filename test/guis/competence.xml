<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../xml/setup.xsl"?>
<gui xmlns="uri:hui" pad="10" title="Box">
	<box width="400" padding="10" top="100" variant="textured">
		<formula name="formula">
			<header>Ny begvienhed</header>
			<space height="5"/>
			<group>
				<text label="Betegnelse:"/>
				<text label="Sted:"/>
				<dropdown label="Type:">
					<item title="Anden" value="0"/>
					<item title="Arbejdsfunktion" value="0"/>
				</dropdown>
				<field label="Startdato:">
					<datetime/>
				</field>
				<field label="Slutdato:">
					<datetime/>
				</field>
				<buttons top="5">
					<button title="Annuller" action="hui.log(formula.getValues());"/>
					<button title="Opret" highlighted="true"/>
				</buttons>
			</group>
		</formula>
	</box>
</gui>