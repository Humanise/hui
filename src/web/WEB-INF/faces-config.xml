<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd" 
    version="2.0">
    <application>
   		<variable-resolver>
      		org.springframework.web.jsf.DelegatingVariableResolver
   		</variable-resolver>
	</application>
	<managed-bean>
        <managed-bean-name>base</managed-bean-name>
        <managed-bean-class>dk.in2isoft.onlineobjects.apps.community.jsf.BaseBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
	<managed-bean>
        <managed-bean-name>images</managed-bean-name>
        <managed-bean-class>dk.in2isoft.onlineobjects.apps.community.jsf.ImagesBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
	<managed-bean>
        <managed-bean-name>users</managed-bean-name>
        <managed-bean-class>dk.in2isoft.onlineobjects.apps.community.jsf.UsersBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
	<managed-bean>
        <managed-bean-name>userProfile</managed-bean-name>
        <managed-bean-class>dk.in2isoft.onlineobjects.apps.community.jsf.UserProfileBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
	<managed-bean>
        <managed-bean-name>passwordRecovery</managed-bean-name>
        <managed-bean-class>dk.in2isoft.onlineobjects.apps.community.jsf.PasswordRecoveryBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <component>
    	<component-type>onlineobjects.list</component-type>
    	<component-class>dk.in2isoft.onlineobjects.ui.jsf.ListComponent</component-class>
  	</component>
    <component>
    	<component-type>onlineobjects.box</component-type>
    	<component-class>dk.in2isoft.onlineobjects.ui.jsf.BoxComponent</component-class>
  	</component>
    <component>
	    <component-type>in2igui.button</component-type>
    	<component-class>dk.in2isoft.in2igui.jsf.ButtonComponent</component-class>
  	</component>
    <component>
	    <component-type>in2igui.textfield</component-type>
    	<component-class>dk.in2isoft.in2igui.jsf.TextFieldComponent</component-class>
  	</component>
</faces-config>