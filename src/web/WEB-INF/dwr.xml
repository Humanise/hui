<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN" "http://www.getahead.ltd.uk/dwr//dwr20.dtd">
<dwr>
  <allow>
    <create creator="new" javascript="JDate">
      <param name="class" value="java.util.Date"/>
    </create>
    <create creator="new" javascript="CoreSecurity">
      <param name="class" value="dk.in2isoft.onlineobjects.remoting.SecurityRemotingFacade"/>
    </create>
    <create creator="new" javascript="Desktop">
      <param name="class" value="dk.in2isoft.onlineobjects.apps.desktop.RemotingFacade"/>
    </create>
    <create creator="new" javascript="CommunityTool">
      <param name="class" value="dk.in2isoft.onlineobjects.apps.community.RemotingFacade"/>
    </create>
    <create creator="new" javascript="ImageGalleryDocument">
      <param name="class" value="dk.in2isoft.onlineobjects.publishing.imagegallery.ImageGalleryRemotingFacade"/>
    </create>
    <convert converter="bean" match="dk.in2isoft.onlineobjects.apps.desktop.SearchResult"/>
    <convert converter="bean" match="dk.in2isoft.onlineobjects.apps.desktop.Window"/>
    <convert converter="bean" match="dk.in2isoft.onlineobjects.apps.desktop.EntityWindow"/>
    <convert converter="bean" match="dk.in2isoft.onlineobjects.apps.desktop.WindowProperty"/>
    <convert converter="bean" match="dk.in2isoft.onlineobjects.ui.AsynchronousProcessDescriptor"/>
    <convert converter="bean" match="dk.in2isoft.onlineobjects.model.*"/>
    <convert converter="bean" match="dk.in2isoft.in2igui.data.*"/>
  </allow>
</dwr>