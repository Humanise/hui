<?xml version="1.0" encoding="UTF-8"?>
<gui xmlns="uri:hui" title="Scheduler" padding="10">
	<dwr base="/dwr/">
		<interface name="AppSetup"/>
	</dwr>
	<controller name="controller" source="scheduler.js"/>
	<source url="listJobs" name="listSource"/>
	<structure>
		<top>
			<toolbar>
				<icon icon="common/refresh" title="Opdater" name="refresh"/>
				<icon icon="common/stop" title="Stop" name="stop" disabled="true"/>
				<icon icon="common/play" title="Start" name="start"/>
			</toolbar>
		</top>
		<middle>
			<overflow>
				<list name="list" source="listSource"/>
			</overflow>
		</middle>
	</structure>				
</gui>