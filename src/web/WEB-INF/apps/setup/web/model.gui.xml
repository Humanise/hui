<?xml version="1.0" encoding="UTF-8"?>
<gui xmlns="uri:In2iGui" title="Indstillinger">
	<dwr base="/dwr/">
		<interface name="AppSetup"/>
		<interface name="Common"/>
	</dwr>
	<controller name="mainController" source="main.js"/>
	<controller name="controller" source="model.js"/>
	<view>
		<content pad="10">
			<view style="box">
				<toolbar>
					<search/>
				</toolbar>
				<content background="true">
					<split>
						<sidebar>
							<selection value="all" name="selection">
								<item icon="common/object" title="Alle klasser" value="all"/>
								<items name="classes"/>
							</selection>
						</sidebar>
						<content>
							<overflow resize="-108">
								<list name="list">
									<column title="Navn" key="name"/>
									<column title="Type" key="type"/>
									<column title="Oprettet" key="created"/>
									<column title="Opdateret" key="updated"/>
								</list>
							</overflow>
						</content>
					</split>
				</content>
				<foot/>
			</view>
		</content>
	</view>
</gui>