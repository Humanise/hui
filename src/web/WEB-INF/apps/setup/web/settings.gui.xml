			<tab title="Administrator">
				<formula name="formula">
					<space all="10">
						<horizontal space="20">
							<content>
								<space all="1"/>
							</content>
							<content>
								<header>Skift adgangskode</header>
								<group labels="above">
									<text secret="true" label="Adgangskode" name="password1"/>
									<text secret="true" label="Gentag adgangskode" name="password2"/>
									<buttons>
									<button title="Skift" name="changePassword"/>
