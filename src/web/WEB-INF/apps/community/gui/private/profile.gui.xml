<?xml version="1.0" encoding="UTF-8"?>
<gui xmlns="uri:In2iGui" context="../../../../">
	<dwr base="../../../../dwr/">
		<interface name="CoreSecurity"/>
		<interface name="CommunityTool"/>
	</dwr>
	<controller name="controller" source="../../gui/private/profile.js"/>
	<tabbox>
		<tab title="Personligt">
			<formula name="personFormula">
				<horizontal>
					<content>
						<group>
							<text label="Præfiks" name="namePrefix"/>
							<text label="Fornavn" name="givenName"/>
							<text label="Mellemnavn" name="familyName"/>
							<text label="Suffiks" name="nameSuffix"/>
						</group>
					</content>
					<content>
						<group>
							<text label="Brugernavn" name="username"/>
						</group>
					</content>
				</horizontal>
				<group>
					<button title="Gem" name="savePerson"/>
				</group>
			</formula>
		</tab>
		<tab title="Beskeder">
			<view>
				<toolbar>
					<icon icon="common/smiley" title="Ny person" name="newPerson"/>
					<icon icon="common/folder" title="Open"/>
					<search title="Søgning" object="search"/>
					<icon icon="common/folder" title="Icons" name="changeToIconView"/>
					<icon icon="common/folder" title="List" name="changeToListView"/>
				</toolbar>
				<content>
					<tabs>
						<tab title="Egenskaber">
						</tab>
						<tab title="Links">
			
						</tab>
					</tabs>
				</content>
			</view>
		</tab>
	</tabbox>
</gui>
