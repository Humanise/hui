<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="setup.xsl"?>
<gui xmlns="uri:In2iGui" pad="10" title="Formula">
	<controller name="controller" source="formula.js"/>
	<box width="400" title="Title with toolbar">
		<toolbar>
			<icon icon="common/color" title="Hep hey!" selected="true" action="alert(0)"/>
			<icon icon="common/folder" title="Show values" name="showValues"/>
			<icon icon="common/settings" title="Hep hey!" disabled="true"/>
		</toolbar>
		<tabs>
			<tab title="Formula 1" padding="10">
				<formula name="formula1">
					<group labels="above">
						<text label="Brugernavn:" key="username"/>
						<password label="Kodeord:" key="password"/>
						<text label="Notat:" lines="6"/>
						<checkbox label="Intern adgang:"/>
						<checkbox label="Ekstern adgang:"/>
						<checkboxes label="Grupper:" key="groups">
							<source url="source_checkboxes.xml"/>
						</checkboxes>
						<tokens label="Tokens:"/>
						<buttons top="10">
							<button title="Gem" highlighted="true"/>
							<button title="Slet"/>
							<button title="Annuller"/>
						</buttons>
					</group>
				</formula>
			</tab>
			<tab title="Formula 2">
				<formula name="formula2">
					<group>
						<text label="Brugernavn:" key="username"/>
						<password label="Kodeord:" key="password"/>
						<text label="Notat:" lines="6"/>
						<checkbox label="Intern adgang:"/>
						<checkbox label="Ekstern adgang:"/>
						<tokens label="Tokens:"/>
						<buttons top="10">
							<button title="Gem" highlighted="true"/>
							<button title="Slet"/>
							<button title="Annuller"/>
						</buttons>
					</group>
				</formula>
			</tab>
		</tabs>
	</box>
</gui>